{
  "author": "Marcel Gleeson <gleeson.marcel@gmail.com>",
  "name": "sobaka-sample",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "cd ../audio-worklet && npm run build",
    "build": "rimraf dist pkg && webpack --stats-children",
    "start": "rimraf dist pkg && webpack serve --stats-children --open",
    "postinstall": "cd ../audio-worklet && npm install",
    "postci": "cd ../audio-worklet && npm ci",
    "format": "prettier --no-error-on-unmatched-pattern --write \"./src/**/*.{js,svelte,ts}\"",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "postlint": "cd ../audio-worklet && npm run lint",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/lodash": "^4.14.178",
    "@types/webmidi": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-svelte3": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "postcss": "^8.4.7",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^14.0.2",
    "postcss-load-config": "^3.1.3",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rimraf": "^3.0.0",
    "source-map-loader": "^3.0.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.4",
    "ts-loader": "^9.2.3",
    "tslib": "^2.3.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.0.0",
    "webpack-merge": "^5.0.0",
    "worklet-loader": "^1.0.0"
  },
  "dependencies": {
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "ress": "^4.0.0",
    "sobaka-sample-audio-worklet": "file:../audio-worklet",
    "svelte": "^3.45.0",
    "svelte-routing": "^1.6.0",
    "typescript": "^4.3.4"
  }
}
