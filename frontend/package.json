{
  "author": "Marcel Gleeson <gleeson.marcel@gmail.com>",
  "name": "sobaka-sample",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prebuild": "cd ../sobaka-dsp && npm run build",
    "postinstall": "cd ../sobaka-dsp && npm install",
    "postci": "cd ../sobaka-dsp && npm ci",
    "postlint": "cd ../sobaka-dsp && npm run lint",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.41",
    "@sveltejs/kit": "1.0.0-next.461",
    "@types/lodash": "^4.14.178",
    "@types/webmidi": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.1.0-beta.1"
  },
  "dependencies": {
    "@syncedstore/core": "^0.4.3",
    "@syncedstore/svelte": "^0.4.3",
    "date-fns": "^2.29.2",
    "events": "^3.3.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "ress": "^4.0.0",
    "sobaka-dsp": "file:../sobaka-dsp",
    "webmidi": "^3.0.21",
    "y-indexeddb": "^9.0.9",
    "y-webrtc": "^10.2.4",
    "yjs": "^13.5.48"
  }
}
