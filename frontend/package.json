{
  "author": "Marcel Gleeson <gleeson.marcel@gmail.com>",
  "name": "sobaka-sample",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "cd web-audio && npm run build",
    "build": "rimraf dist pkg && webpack --stats-children",
    "start": "rimraf dist pkg && webpack serve --stats-children --open",
    "postinstall": "cd web-audio && npm install",
    "format": "prettier --no-error-on-unmatched-pattern --write \"./src/**/*.{js,svelte,ts}\"",
    "lint": "eslint './src/**/*.{js,ts,svelte}'",
    "postlint": "cd web-audio && npm run lint",
    "lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-plugin-svelte3": "^3.3.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "rimraf": "^3.0.0",
    "source-map-loader": "^3.0.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.3",
    "ts-loader": "^9.2.3",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "worklet-loader": "^1.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "sobaka-sample-web-audio": "file:web-audio",
    "svelte": "^3.45.0",
    "svelte-routing": "^1.6.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4",
    "webaudio-oscilloscope": "^3.2.1"
  }
}
