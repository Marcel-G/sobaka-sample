{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "compilerOptions": {
    "importHelpers": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "importsNotUsedAsValues": "remove",
    "strict": true,
    "module": "ESNext"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
}
